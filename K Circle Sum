static void  kCircleSum(int arr[],int n,int k){
//Enter your code here
//int temp = arr[n-1];
//int temp = arr[0];
int sum=0;
for(int i =0; i<k;i++)
{
    sum+=arr[i];
}
for(int i=k;i<n+k;i++)
{
System.out.print(sum+" ");
sum+=arr[i%n]-arr[(i-k)%n];
// int newsum=0;
// for(int i=n-k+1 ;i<n;i++)
// {
//  newsum+=arr[i];
// }
// System.out.print(temp+newsum);
}
}
